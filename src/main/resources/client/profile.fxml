<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" style="-fx-background-color: #e5eefc;"
            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="client.controllers.ProfileController">
    <children>
        <Label layoutX="180.0" layoutY="20.0" text="Профиль" textAlignment="CENTER">
            <font>
                <Font size="24.0" />
            </font>
        </Label>

        <Label layoutX="50.0" layoutY="80.0" text="Имя:"/>
        <Label fx:id="firstNameLabel" layoutX="150.0" layoutY="80.0"/>

        <Label layoutX="50.0" layoutY="110.0" text="Фамилия:"/>
        <Label fx:id="lastNameLabel" layoutX="150.0" layoutY="110.0"/>

        <Label layoutX="50.0" layoutY="140.0" text="Логин:"/>
        <Label fx:id="loginLabel" layoutX="150.0" layoutY="140.0"/>

        <Button fx:id="backButton" layoutX="180.0" layoutY="235.0" mnemonicParsing="false" onAction="#backToMain" prefHeight="37.0" prefWidth="100.0"
                style="-fx-background-color: #e5eefc; -fx-border-color: #c7dcf7; -fx-border-width: 3px; -fx-border-radius: 10px;" text="Назад" textAlignment="CENTER">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
